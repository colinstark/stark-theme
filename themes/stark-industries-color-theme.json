	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "string.comment"],
			"settings": {
				"foreground": "#555555"
			}
		},
		{
			"name": "String",
			"scope": ["string", "string.template"],
			"settings": {
				"foreground": "#ffc062"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#908CFF"
			}
		},
		{
			"name": "Numbers in CSS",
			"scope": ["constant.numeric.css", "constant.numeric.scss"],
			"settings": {
				"foreground": "#5de4ae"
			}
		},
		{
			"name": "Embedded String Begin/End",
			"scope": [
				"string.embedded.begin",
				"string.embedded.end",
				"punctuation.definition.template-expression",
				"punctuation.section.embedded",
				"meta.import.tsx"
			],
			"settings": {
				"foreground": "#FC3768"
			}
		},
		{
			"name": "Default Embedded Color",
			"scope": [
				"punctuation.section.embedded.begin.js",
				"punctuation.section.embedded.end.js",
				"punctuation.section.embedded.begin.erb",
				"punctuation.section.embedded.end.erb",
				"source.elixir.embedded",
				"punctuation.separator",
				"punctuation.accessor",
				"meta.brace",
				"constant.language.import-export-all"
			],
			"settings": {
				"foreground": "#cabdaa"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant.language",
				"support.class.builtin",
				"constant.character",
				"constant.other",
				"support.type.primitive"
			],
			"settings": {
				"foreground": "#FC3768"
			}
		},
		{
			"name": "Language Variable",
			"scope": ["variable.language", "keyword.control.flow", "support.function.builtin"],
			"settings": {
				"foreground": "#FC3768"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "meta.import"],
			"settings": {
				"foreground": "#FC3768"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator.logical",
				"keyword.operator.constructor",
				"keyword.operator.ternary",
				"keyword.control.conditional",
				"keyword.operator.comparison",
				"keyword.operator.relational",
				"keyword.control.trycatch"
			],
			"settings": {
				"foreground": "#5de4ae"
			}
		},
		{
			"name": "Control flow",
			"scope": ["keyword.control.flow", "storage.modifier.async"],
			"settings": {
				"foreground": "#908CFF"
			}
		},
		{
			"name": "Console",
			"scope": ["support.class.console"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#908CFF"
			}
		},
		{
			"name": "Booleans",
			"scope": ["constant.language.boolean"],
			"settings": {
				"fontStyle": "",
				"foreground": "#908CFF"
			}
		},

		{
			"name": "Keyword Operator",
			"scope": ["keyword.operator", "storage.type.function.arrow"],
			"settings": {
				"fontStyle": "",
				"foreground": "#cabdaa"
			}
		},
		{
			"name": "Arrow",
			"scope": ["storage.type.function.arrow"],
			"settings": {
				"foreground": "#908CFF"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#dba88d"
			}
		},
		{
			"name": "Modifiers",
			"scope": "storage.modifier",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#cabdaa88"
			}
		},

		{
			"name": "Types",
			"scope": "storage.type",
			"settings": {
				// "fontStyle": "italic",
				"foreground": "#FC3768"
			}
		},
		{
			"name": "Class",
			"scope": ["entity.name.class", "entity.name.module", "entity.name.type", "storage.identifier", "support.class"],
			"settings": {
				"foreground": "#FC3768"
			}
		},
		{
			"name": "Variable Object",
			"scope": [
				"variable.other.object",
				"variable.other.constant",
				"variable.other.global",
				"variable.other.readwrite.class",
				"variable.other.readwrite.instance",
				"variable.other.readwrite.batchfile",
				"variable.other.readwrite",
				"variable.other.php",
				"variable.readwrite",
				"variable.readwrite.other.block",
				"entity.name.function.decorator"
			],
			"settings": {
				"foreground": "#84c6f1"
			}
		},
		{
			"name": "Other variable",
			"scope": [
				"variable.other",
				"variable.other.property",
				"variable.other.block",
				"entity.name.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#cabdaa"
			}
		},
		{
			"name": "Inherited Class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#84c6f1"
			}
		},
		{
			"name": "Package Import & Export",
			"scope": [
				"storage.modifier.import",
				"storage.modifier.package",
				"keyword.control.import",
				"keyword.control.from",
				"keyword.control.export",
				"keyword.control.default"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FC3768"
			}
		},
		{
			"name": "Function name",
			"scope": ["entity.name.function", "support.function", "meta.function-call"],
			"settings": {
				"foreground": "#84c6f1"
			}
		},
		// {
		// 	"name": "Function keyword",
		// 	"scope": ["storage.type.function."],
		// 	"settings": {
		// 		"foreground": "#84c6f1"
		// 	}
		// },
		{
			"name": "Function argument",
			"scope": ["variable.parameter", "entity.name.variable.parameter", "parameter.variable"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#cabdaa99"
			}
		},
		{
			"name": "Function call",
			"scope": "entity.name.function-call",
			"settings": {
				"foreground": "#FC3768"
			}
		},
		{
			"name": "Builtin Functions",
			"scope": ["function.support.builtin", "function.support.core"],
			"settings": {
				"foreground": "#5DE4AE"
			}
		},
		{
			"name": "Tag Name",
			"scope": [
				"entity.name.tag",
				"entity.name.tag.class.js",
				"entity.name.tag",
				"entity.name.tag.class.js",
				"entity.name.tag.class.jsx",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end"
			],
			"settings": {
				"foreground": "#cabdaa"
			}
		},
		{
			"name": "Special tags",
			"scope": [
				"entity.name.tag.template.html",
				"entity.name.tag.script.html",
				"entity.name.tag.style.html"
				// "support.class.component.js"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},

		{
			"name": "Tag Class and ID",
			"scope": ["entity.name.tag.class", "entity.name.tag.id", "entity.name.type.class"],
			"settings": {
				"foreground": "#84c6f1"
			}
		},
		{
			"name": "Tag Attribute",
			"scope": ["meta.tag.block.any", "x - entity.other.attribute-name"],
			"settings": {
				"foreground": "#FC3768"
			}
		},
		{
			"name": "Directives (Vue etc)",
			"scope": ["meta.directive", "meta.directive.vue"],
			"settings": {
				"foreground": "#5de4ae"
			}
		},

		{
			"name": "Library constant",
			"scope": "support.constant",
			"settings": {
				"foreground": "#84c6f1"
			}
		},
		{
			"name": "Library class/type",
			"scope": ["support.type", "support.variable"],
			"settings": {
				"foreground": "#cabdaa"
			}
		},
		{
			"name": "Json Property",
			"scope": ["support.dictionary.json", "meta.object-literal.key.js"],
			"settings": {
				"foreground": "#cabdaa"
			}
		},
		{
			"name": "StyleSheet Property name",
			"scope": [
				"support.type.property-name.css",
				"support.type.property-name.scss",
				"support.type.property-name.less",
				"support.type.property-name.sass"
			],
			"settings": {
				"foreground": "#cabdaa"
			}
		},
		{
			"name": "CSS: Pseudo Attribute Names",
			"scope": [
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-class.scss",
				"entity.other.attribute-name.pseudo-class.less",
				"entity.other.attribute-name.pseudo-class.sass",
				"entity.other.attribute-name.pseudo-element.css",
				"entity.other.attribute-name.pseudo-element.scss",
				"entity.other.attribute-name.pseudo-element.less",
				"entity.other.attribute-name.pseudo-element.sass"
			],
			"settings": {
				"foreground": "#cabdaabb"
			}
		},
		{
			"name": "StyleSheet Property value",
			"scope": ["support.constant.css", "support.constant.scss", "support.constant.less", "support.constant.sass"],
			"settings": {
				"foreground": "#908CFF"
			}
		},
		{
			"name": "StyleSheet Variable",
			"scope": ["variable.css", "variable.scss", "variable.less", "variable.sass", "entity.name.tag.reference"],
			"settings": {
				"foreground": "#FC3768"
			}
		},
		{
			"name": "StyleSheet Variable String",
			"scope": ["variable.css.string", "variable.scss.string", "variable.less.string", "variable.sass.string"],
			"settings": {
				"foreground": "#ffc062"
			}
		},
		{
			"name": "StyleSheet Unit",
			"scope": ["unit.css", "unit.scss", "unit.less", "unit.sass", "keyword.other.unit"],
			"settings": {
				"foreground": "#FC3768"
			}
		},
		{
			"name": "StyleSheet Function",
			"scope": ["function.css", "function.scss", "function.less", "function.sass"],
			"settings": {
				"foreground": "#5de4ae"
			}
		},
		{
			"name": "Library variable",
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "JSON String",
			"scope": "support.type.property-name.json",
			"settings": {
				"foreground": "#cabdaa"
			}
		},
		{
			"name": "Link",
			"scope": "string.detected-link",
			"settings": {
				"foreground": "#84c6f1"
			}
		},
		{
			"name": "diff.header",
			"scope": ["meta.diff", "meta.diff.header"],
			"settings": {
				"foreground": "#75715E"
			}
		},
		{
			"name": "diff.deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#FC3768"
			}
		},
		{
			"name": "diff.inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#98c379"
			}
		},
		{
			"name": "diff.changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#e5c07b"
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#56b6c2A0"
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#e5c07b"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic, markup.italic.markdown",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Punctuation",
			"scope": [
				"punctuation.definition.italic.markdown",
				"punctuation.definition.bold.markdown",
				"punctuation.definition.heading.markdown"
			],
			"settings": {
				"foreground": "#676f7d"
			}
		},
		{
			"name": "Markup: Emphasis Punctuation",
			"scope": "punctuation.definition.italic.markdown",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markdown: Link",
			"scope": "markup.underline.link.markdown",
			"settings": {
				"foreground": "#84C6F1"
			}
		},
		{
			"name": "Markup: Bold",
			"scope": "markup.bold.markdown",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading.markdown",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#cabdaa"
			}
		},
		{
			"name": "Markup: Quote",
			"scope": "markup.quote.markdown",
			"settings": {
				"foreground": "#908CFF"
			}
		},
		{
			"name": "Markup: Separator",
			"scope": "meta.separator.markdown",
			"settings": {
				"foreground": "#CABDAA",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Raw",
			"scope": ["markup.raw.inline.markdown", "markup.raw.block.markdown"],
			"settings": {
				"foreground": "#84C6F1"
			}
		},
		{
			"name": "Markup: List Punctuation",
			"scope": "punctuation.definition.list_item.markdown",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffffff"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name",
				"keyword.control.at-rule"
			],
			"settings": {
				"foreground": "#5DE4AE"
			}
		},
		{
			"name": "CSS Important",
			"scope": ["keyword.other.important.css", "keyword.other.important.scss", "keyword.other.important.scss"],
			"settings": {
				"foreground": "#FC3768"
			}
		},
		{
			"name": "CSS IDs",
			"scope": ["entity.other.attribute-name.id.css", "entity.other.attribute-name.id.scss"],
			"settings": {
				"foreground": "#ffffff"
			}
		},

		{
			"name": "CSS Classes",
			"scope": ["entity.other.attribute-name.class"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "CSS Toplevel",
			"scope": ["entity.name.tag.css", "entity.name.tag.wildcard.css", "entity.name.tag.wildcard.scss"],
			"settings": {
				"foreground": "#ffffff"
			}
		},

		{
			"name": "StyleSheet Variable",
			"scope": ["variable.other.readwrite.css", "variable.other.readwrite.scss"],
			"settings": {
				"foreground": "#908CFF"
			}
		},

		{
			"name": "expressions",
			"scope": ["meta.var"],
			"settings": {
				"foreground": "#acacac"
			}
		}
	]
}
